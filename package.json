    "sortablejs": "^1.12.0",
    "tailwindcss": "^1.9.6",
    "tailwindcss-shadow-outline-colors": "^0.0.8",
    "vue": "^2.5.17",
    "vue-apexcharts": "^1.6.0",
    "vue-clickaway": "^2.2.2",
    "vue-i18n": "^8.24.1",
    "vue-router": "^3.4.8",
    "vue-screen": "^1.5.4",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
